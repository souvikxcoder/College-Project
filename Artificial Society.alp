					<Properties Width="190" Height="60">
						<ExitAction><![CDATA[int ctr=0;
if(boyFatherWeight*boyFatherRelation!=0)
	ctr=ctr+1;
if(boySchoolWeight*boySchoolRelation!=0)
	ctr=ctr+1;	
if(boyMotherWeight*boyMotherRelation!=0)
	ctr=ctr+1;
if(boyCharacterRelation*boyCharacterWeight!=0)
	ctr=ctr+1;
/*=============Boy to boy===========*/
double sumgb=0;
for (Map.Entry mapElement : boyBoyRelation.entrySet()) 
    sumgb+=(double)mapElement.getValue();
boyImpact=boyBoyWeight*sumgb;
if(boyImpact!=0)
	boyImpact/=boyBoyRelation.size();
if(boyImpact!= 0)
	ctr=ctr+1;	
/*=============Boy to Girl===========*/
double sumgg=0;
for (Map.Entry mapElement : boyGirlRelation.entrySet()) 
    sumgg+=(double)mapElement.getValue();
girlImpact=boyGirlWeight*sumgg;
if(girlImpact!=0)
	girlImpact/=boyGirlRelation.size();
if(girlImpact!= 0)
	ctr=ctr+1;

/*=============Boy to Teacher=========*/
double sum=0;
for (Map.Entry mapElement : boyTeacherRelation.entrySet()) 
    sum+=(double)mapElement.getValue();
