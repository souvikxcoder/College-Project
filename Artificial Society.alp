					<Parameter>
						<Name><![CDATA[characterFather]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Body><![CDATA[if(Math.abs(charVariableFather-charVariableMother)<=0.05) 
{ 
	if(charVariableFather>=charVariableMother) 
	{
		return uniform(charVariableMother,charVariableFather);
	} 
	else 
	{ 
		return uniform(charVariableFather,charVariableMother);
	} 
} 
else if(Math.abs(charVariableFather-charVariableMother)<=0.15)
{ 
	double z=max(charVariableFather,charVariableMother); 
	double diff=abs(charVariableFather-charVariableMother); 
	
	return uniform(z-diff,min(1,z+diff));
}
else
{
	double z=min(charVariableFather,charVariableMother); 
	double diff=abs(charVariableFather-charVariableMother); 
	return uniform(max(z-diff,0),z+diff);
}]]></Body>
