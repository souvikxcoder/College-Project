for(int i=0;i<teacherToGirlCount;i++)
	girlWeightT0+=this.teacherToGirl.getConnectedAgent(i).totalWeightInitial;
double teacherGirlVariable=teacherGirlSlider;
relationPerformanceGT0=girlImpact/totalWeightInitial;
relationPerformanceGT1=((girlImpact*girlWeightT1)/girlWeightT0)/totalWeight;
Map< Integer,Double> relationPreVariableg = new HashMap< Integer,Double>(); 
for(int i=0;i<teacherToGirlCount;i++)
	if(teacherGirlRelationInitial.containsKey(this.teacherToGirl.getConnectedAgent(i).idg))
	{
		double girlToTeacherWeight=this.teacherToGirl.getConnectedAgent(i).girlTeacherWeight;
		if(relationPerformanceGT1 >relationPerformanceGT0)
		{
			double relVal=min(1.0,(teacherGirlRelationInitial.get(this.teacherToGirl.getConnectedAgent(i).idg)+ defaultChange*teacherChange/maxChange)+(defaultChange+girlToTeacherWeight*defaultChange*teacherGirlRelationInitial.get(this.teacherToGirl.getConnectedAgent(i).idg)));
			relationPreVariableg.put(this.teacherToGirl.getConnectedAgent(i).idg,max(-1,relVal));
			/*======================teacher girl SLIDER======================*/
			if(teacherGirlSlider!=0)
				relationPreVariableg.put(this.teacherToGirl.getConnectedAgent(i).idg,max(-1,relationPreVariableg.get(this.teacherToGirl.getConnectedAgent(i).idg)+teacherGirlVariable));	
		}
		else if(relationPerformanceGT1<relationPerformanceGT0)
		{
			double relVal=max(-1.0,(teacherGirlRelationInitial.get(this.teacherToGirl.getConnectedAgent(i).idg)+ defaultChange*teacherChange/maxChange)-(defaultChange-girlToTeacherWeight*defaultChange*teacherGirlRelationInitial.get(this.teacherToGirl.getConnectedAgent(i).idg)));
			relationPreVariableg.put(this.teacherToGirl.getConnectedAgent(i).idg,min(1,relVal));
			/*======================teacher girl SLIDER======================*/
			if(teacherGirlSlider!=0)
				relationPreVariableg.put(this.teacherToGirl.getConnectedAgent(i).idg,min(1,relationPreVariableg.get(this.teacherToGirl.getConnectedAgent(i).idg)+teacherGirlVariable));	
		}
		else
		{
			relationPreVariableg.put(this.teacherToGirl.getConnectedAgent(i).idg,teacherGirlRelationInitial.get(this.teacherToGirl.getConnectedAgent(i).idg));
			if(teacherTeacherSlider!=0)
			{
				double preValueTeacherGirl;
				preValueTeacherGirl=teacherGirlRelationInitial.get(this.teacherToGirl.getConnectedAgent(i).idg)+teacherGirlVariable;
				if(preValueTeacherGirl>1)
					preValueTeacherGirl=1;
				else if(preValueTeacherGirl<-1)	
					preValueTeacherGirl=-1;
				relationPreVariableg.put(this.teacherToGirl.getConnectedAgent(i).idg,preValueTeacherGirl);
			}
		
		}
	}
	else
		relationPreVariableg.put(this.teacherToGirl.getConnectedAgent(i).idg,2.0);
